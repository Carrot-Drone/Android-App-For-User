<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:views="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/listview_height">

    <com.lchpartners.views.NamsanTextView
        android:id="@+id/text_view_version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:textSize="19.5sp"
        android:textColor="@android:color/black"
    />

    <com.lchpartners.views.NamsanTextView
        android:id="@+id/text_view_config_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/text_view_version"
        views:weight="light"
        android:textSize="19.5sp"
        android:textColor="@android:color/black"
    />

</RelativeLayout>